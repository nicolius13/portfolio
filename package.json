    "nuxt": "^2.4.0",
    "nuxt-fontawesome": "^0.4.0",
    "nuxt-i18n": "^6.13.1",
    "rfs": "^9.0.3",
    "vue-typed-js": "^0.1.2"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "@nuxtjs/eslint-config": "^0.0.1",
    "@nuxtjs/style-resources": "^1.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.15.1",
