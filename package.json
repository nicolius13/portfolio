{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "My superb portfolio",
  "author": "Nicolas Vastrade",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "build:gh-pages": "cross-env DEPLOY_ENV=GH_PAGES nuxt build",
